<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.gph.saviorframework.common.model">
	<class name="User" table="SF_USER" dynamic-update="true" dynamic-insert="true">
		<comment>用户表</comment>
		<id name="username" length="50">
			<column name="USER_ID">
				<comment>用户名</comment>
			</column>
		</id>
		<version name="version" type="integer">
			<column name="USER_VERSION">
				<comment>数据版本</comment>
			</column>
		</version>
		<property name="name">
			<column name="USER_NAME" length="100">
				<comment>姓名</comment>
			</column>
		</property>
		<property name="description">
			<column name="USER_DESCRIPTION" length="500">
				<comment>描述</comment>
			</column>
		</property>
		<property name="password">
			<column name="USER_PASSWORD" length="100">
				<comment>密码</comment>
			</column>
		</property>
		<property name="gender">
			<column name="USER_GENDER" length="100">
				<comment>性别</comment>
			</column>
		</property>
		<property name="identity">
			<column name="USER_ID_NO" length="100" unique="true">
				<comment>身份证号</comment>
			</column>
		</property>
		<property name="email">
			<column name="USER_EMAIL" length="100">
				<comment>邮件</comment>
			</column>
		</property>
		<property name="address">
			<column name="USER_ADDRESS" length="500">
				<comment>地址</comment>
			</column>
		</property>
		<property name="post">
			<column name="USER_POST" length="10">
				<comment>邮编</comment>
			</column>
		</property>
		<property name="birthday" type="date">
			<column name="USER_BIRTHDAY">
				<comment>生日</comment>
			</column>
		</property>
		<property name="tel">
			<column name="USER_TEL" length="100">
				<comment>电话</comment>
			</column>
		</property>
		<property name="mobile">
			<column name="USER_MOBILE" length="100">
				<comment>手机</comment>
			</column>
		</property>
		<property name="order" type="integer">
			<column name="USER_ORDER">
				<comment>顺序</comment>
			</column>
		</property>
		<many-to-one name="position" class="Position" lazy="false">
			<column name="POSITION_ID">
				<comment>岗位</comment>
			</column>
		</many-to-one>
		<property name="ext">
			<column name="USER_EXT" length="100">
				<comment>头像</comment>
			</column>
		</property>
		<property name="enabled" type="boolean">
			<column name="USER_ENABLED">
				<comment>启用标志</comment>
			</column>
		</property>
		<property name="accountNonExpired" type="boolean">
			<column name="USER_ACCOUNT_NONEXPIRED">
				<comment>未过期</comment>
			</column>
		</property>
		<property name="accountNonLocked" type="boolean">
			<column name="USER_ACCOUNT_NONLOCKED">
				<comment>未锁定</comment>
			</column>
		</property>
		<property name="credentialsNonExpired" type="boolean">
			<column name="USER_CREDENTIALS_NONEXPIRED">
				<comment>凭证未过期</comment>
			</column>
		</property>
		<set name="roles" table="SF_ROLE_USER" lazy="false">
			<key column="USER_ID" />
			<many-to-many class="Role">
				<column name="ROLE_ID">
					<comment>角色ID</comment>
				</column>
			</many-to-many>
		</set>
		<many-to-one name="org" class="Org" lazy="false">
			<column name="ORG_ID">
				<comment>所属机构</comment>
			</column>
		</many-to-one>
		<many-to-one name="creator" class="User" fetch="join" update="false">
			<column name="USER_CREATOR">
				<comment>创建者</comment>
			</column>
		</many-to-one>
		<property name="created" type="date" update="false">
			<column name="USER_CREATED">
				<comment>创建时间</comment>
			</column>
		</property>
		<many-to-one name="modifier" class="User" fetch="join">
			<column name="USER_MODIFIER">
				<comment>修改者</comment>
			</column>
		</many-to-one>
		<property name="modified" type="date">
			<column name="USER_MODIFIED">
				<comment>修改时间</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>